{
  "name": "Scaling Test: 10k â†’ 100k",
  "dataset": {
    "path": "datasets/synthetic/small-64d-10k.bin"
  },
  "queries": {
    "count": 100,
    "k": [10, 50, 100]
  },
  "libraries": [
    {
      "name": "diskann"
    },
    {
      "name": "vec"
    }
  ],
  "diskann": {
    "maxDegree": [32],
    "buildSearchListSize": [100],
    "searchListSize": [150],
    "metric": "cosine",
    "normalizeVectors": false
  },
  "metrics": {
    "computeRecall": true,
    "measureMemory": false,
    "warmupQueries": 10,
    "groundTruthPath": "datasets/ground-truth/small-64d-10k.json"
  }
}
