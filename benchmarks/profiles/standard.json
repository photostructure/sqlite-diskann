{
  "name": "Standard benchmark",
  "dataset": {
    "path": "datasets/synthetic/medium-256d-100k.bin"
  },
  "queries": {
    "count": 200,
    "k": [1, 10, 50, 100]
  },
  "libraries": [
    {
      "name": "vec"
    },
    {
      "name": "usearch"
    }
  ],
  "usearch": {
    "metric": "euclidean"
  },
  "metrics": {
    "computeRecall": true,
    "measureMemory": true,
    "warmupQueries": 20,
    "groundTruthPath": "datasets/ground-truth/medium-256d-100k.json"
  }
}
